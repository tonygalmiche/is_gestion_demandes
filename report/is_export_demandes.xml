<?xml version="1.0" encoding="utf-8"?><openerp><data>        <record id="is_export_demandes_tree" model="ir.ui.view">        <field name="name">is_export_demandes_tree</field>        <field name="model">is.export.demandes</field>        <field name="arch" type="xml">            <tree>                <field name="name" />                <field name="createur" />                <field name="demandeur" />                <field name="date_demande" />                <field name="application" />                <field name="demande" />                <field name="etude" />                <field name="tps_prevu" />                <field name="date_validation" />                <field name="date_realisation" />                <field name="tps_passe" />                <field name="facture" />                <field name="state" />           </tree>        </field>    </record>    <record id="is_export_demandes_search" model="ir.ui.view">        <field name="name">is_export_demandes_search</field>        <field name="model">is.export.demandes</field>        <field name="arch" type="xml">            <search string="Recherche">                <!-- Filtres disponilbes par défaut -->                <field name="name"/>                <field name="createur"/>                <field name="demandeur"/>                <field name="application"/>                <field name="demande"/>                <field name="etude"/>                <field name="facture"/>                <field name="state"/>                <field name="name" string="Dans tous les champs"                       filter_domain="['|','|','|', ('name','ilike',self), ('demande','ilike',self), ('etude', 'ilike', self), ('facture', 'ilike', self)]"/>                <!-- Group by -->                <!--                <group string="Group By">                    <filter string="N°TVA"        context="{'group_by': 'num_tva'}"       name="group_by_num_tva" />                    <filter string="Date facture" context="{'group_by': 'date_facture'}"  name="group_by_date_facture"/>                </group>                -->            </search>        </field>    </record>    <record id="is_export_demandes_action" model="ir.actions.act_window">        <field name="name">Exportation des demandes</field>        <field name="res_model">is.export.demandes</field>        <field name="view_type">form</field>        <field name="view_mode">tree</field>    </record>    <!--<menuitem action="is_export_demandes_action" id="is_menu_export_demandes" parent="menu_gestion_demandes_indicateurs" sequence="90"/>--></data></openerp>